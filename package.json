{
  "name": "@stacksjs/ts-clone",
  "type": "module",
  "version": "0.1.1",
  "description": "Simple and fast TypeScript in-memory caching. Modern TypeScript port of node-cache.",
  "author": "Chris Breuer <chris@stacksjs.org>",
  "license": "MIT",
  "homepage": "https://github.com/stacksjs/ts-clone#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stacksjs/ts-clone.git"
  },
  "bugs": {
    "url": "https://github.com/stacksjs/ts-clone/issues"
  },
  "keywords": [
    "typescript",
    "cache",
    "caching",
    "memory",
    "internal",
    "local",
    "node",
    "in-memory",
    "library",
    "memcached",
    "object"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./*": {
      "import": "./dist/*"
    }
  },
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": ["README.md", "dist"],
  "scripts": {
    "build": "bun --bun build.ts",
    "lint": "bunx --bun eslint .",
    "lint:fix": "bunx --bun eslint . --fix",
    "fresh": "bunx rimraf node_modules/ bun.lock && bun i",
    "changelog": "bunx changelogen --output CHANGELOG.md",
    "prepublishOnly": "bun --bun run build",
    "release": "bun run changelog && bunx bumpp package.json --all",
    "test": "bun test",
    "typecheck": "bun --bun tsc --noEmit"
  },
  "devDependencies": {
    "@happy-dom/global-registrator": "^17.4.4",
    "@stacksjs/eslint-config": "^4.10.2-beta.3",
    "@types/bun": "^1.2.6",
    "bumpp": "^10.1.0",
    "bun-plugin-dtsx": "^0.21.9",
    "changelogen": "^0.6.1",
    "lint-staged": "^15.5.0",
    "simple-git-hooks": "^2.12.1",
    "typescript": "^5.8.2"
  },
  "lint-staged": {
    "*.{js,ts}": "bunx --bun eslint . --fix"
  }
}
